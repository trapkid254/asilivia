# Copy this file to .env and fill in the values

# MongoDB Atlas connection string
# Example: mongodb+srv://<user>:<pass>@<cluster>/<db>?retryWrites=true&w=majority&appName=<app>
MONGODB_URI=

# Comma-separated list of allowed origins for CORS
# e.g. https://your-netlify-site.netlify.app,https://your-custom-domain.com
CORS_ORIGIN=

# Node port (Render will inject PORT automatically)
PORT=8080

# Admin token for protecting write endpoints (set a strong random string)
ADMIN_TOKEN=
